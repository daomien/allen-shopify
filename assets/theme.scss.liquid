/*==============================================
style menu 
================================================*/
.nav-container{
  border-bottom: 1px solid #e5e5e5;

  #menu-navigation-horizontal{
    background:$menu_background_color !important;
    margin: 0 0 0 -15px;

    ul.site-nav{
      text-align:center;
      margin: 0 !important;

      a.menu-link{
        font-family:$menu_font_google_name;
        font-weight:$menu_font_weight;
        font-size:$menu_font_size;
        border:none !important;
        box-shadow:none !important;
        border-radius:none !important;
        text-decoration:none;
        color:$menu_color_dropdown !important;

        i{
          padding-right:5px;
        }

        &:hover{
          background:$menu_background_color_active !important;
          color: $menu_color_primary_active !important;
          text-decoration:none;
          background-image:url($menu_background_image);
        }

      }
      >li.menu-level-1{
        display: inline-block;
        margin: 0 !important;
        position: relative;

        &:hover {
          ul.dropdown-menu{
            display:inline-block !important;
            @include prefixer( transform, scaleY(1), spec webkit moz o ms );
            @include prefixer( transition, all 0.5s ease-out 0s, spec webkit moz o ms );
          }
        }
        >a{
          color: $menu_color_primary !important;
          padding: 0 27px;
          text-transform: uppercase;
          line-height: 50px;
          margin-right: 1px;
          min-height: 50px;

          &:after{
            border: none;
            color: $colorTextBody;
            content: "\f0d7";
            display: inline-block;
            font-family: FontAwesome;
            font-size: 12px;
            margin: 0;
            position: absolute;
            right: 0px;
            top: 1px;
            width: auto;
          }
          &:hover:after{
            color:$colorPrimary;
          }
        }

        >ul.dropdown-menu{
          left: 0;
          margin:0 !important;
          position: absolute;
          z-index: 9999;
          display: none;
          padding:10px ;
          background-color:$menu_background_color_dropdown;
          padding:10px;
          display: inline-block !important;
          @include prefixer( box-shadow, 0px 4px 13px -3px #808080, spec webkit moz o ms );
          @include prefixer( transform, scaleY(0), spec webkit moz o ms );
          @include prefixer( transition, all 0.5s ease-out 0s, spec webkit moz o ms );
          transform-origin: 0 0 0;

          &:hover{
            display:block;
            @include prefixer( box-shadow, 0px 4px 13px -3px #808080, spec webkit moz o ms );
          }

          li.menu-level-2{
            width:33%;
            float:left;
            padding: 0 10px;
            list-style-type:none !important;

            >a{
              background:$menu_background_color_dropdown !important;
              border-bottom: 1px solid #e5e5e5 !important;
              text-align:left;
              padding:8px 0;
              text-transform: uppercase;
              margin: 0 0 10px;


              &:hover{
                color:$menu_color_dropdown;
                background:$menu_background_color_dropdown;
              }
            }
            ul.dropdown-third {
              margin:0;
              padding:0;
              display:initial;
              li.menu-level-3{
                list-style-type:none !important;
                text-align:left;
                a.menu-link{
                  text-transform:capitalize;
                  border-bottom:none !important;
                  font-weight:400 !important;
                  font-weight:normal;
                  padding:8px 0;
                  color:$menu_color_dropdown;

                }
                &:hover{

                  color: $menu_color_dropdown_active;
                }
              }
            }
          }
        }
      }

      li.active.menu-level-1{
        > a{
          background:$menu_background_color_active !important; 
          color:$menu_color_primary_active !important;
        }
      }

      li:nth-of-type(1){

        ul.dropdown-menu.no-lever-3{

          margin: 0;
          padding:10px;
          z-index: 5;
          width:480px;
          background-color:$menu_background_color_dropdown;

          li.nochild{
            width:50%;
            float:left;
            padding:0 10px;
            list-style-type:none !important;

            a {
              background: $menu_background_color_dropdown !important ;
              border-bottom: 1px solid #e5e5e5 !important;
              margin:0 0 10px;
              text-align:left;
              padding: 8px 0px;
              text-transform: uppercase;;

              &:hover,
              &:active,
              &:focus {
                background: $menu_background_color_dropdown_active !important;
                color: $menu_color_dropdown_active;
              }
            }
          }
        }
      }


      li:nth-of-type(4){
        ul.dropdown-menu.no-lever-3{
          width:255px;
          padding: 0 !important;

          li.nochild{
            width:100%;
            list-style-type:none !important;

            a.menu-link{
              border-bottom: 1px solid #e5e5e5 !important;
              text-transform:initial !important;
              font-weight: normal;
              padding:12px 25px;
              text-align: left !important;
              margin: 0 !important;
            }
            &:last-child{
              a.menu-link{
                border-bottom:none !important;
              }
            }
          }
        }
      }

      li:nth-of-type(5){
        @extend li:nth-of-type(4); 
      }

      li:nth-of-type(6){
        ul.dropdown-menu {
          width:1170px;
          position:absolute;
          left:-720px;
          margin:0;
          li.has_child.menu-level-2{
            width: 20%;
            float: left;
            padding: 0 10px;
            list-style-type:none !important;
          }
        }
      }
    }
  }
}

#menu-navigation-vertical{
  ul.site-nav{
    width: 18%;
    position: absolute;
    z-index: 999;
    background: #dedede;
    >li.menu-level-1{
      display: block;
      position: relative;
      padding: 10px 10px;

      >a{
        padding:10px 30px;
        background:$menu_background_color;

      }
      &:hover{
        ul.dropdown-menu{
          display: block !important;
        }
      }
      >ul.dropdown-menu{
        position: absolute;
        left: 100%;
        top: 0;
        display: none;
        background-color:$menu_background_color_dropdown;
        >li{
          display: block;
          display: inline-block;
          width: 200px;
        }
      }

    }
  }
}


