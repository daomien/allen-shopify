  display: none;
  z-index: 9999;
  line-height: 30px;
  text-align: center;
  @include prefixer( transition, all 0.5s ease 0s, spec webkit moz o ms );

  i{
    vertical-align: middle;
  }
  &:hover{
    text-decoration: none;
    color: #fff;
    background-color:$colorPrimary;
  }
}

/*================Style FOOTER-TOP==============*/
$bg_color_footer_top:  {{ settings.bg_color_footer_top }};
$bg_color_footer_bottom:{{settings.bg_color_footer_bottom}};
.site-footer{
  padding:0  !important;
  .footer-top{
    background: $bg_color_footer_top;
    background-image: url({{ 'bg_footer_top.png' | asset_url }}) no-repeat;
    padding: 20px 0 50px;
    h4{
      margin: 20px 0 30px;
      color: #fff;
      font-size: 16px;
      text-transform: uppercase;
      font-weight: 700;
      line-height: 1.35;
    }
    ul{
      margin: 0;
      padding: 0;

      li{
        line-height: 30px;
        list-style: none;
        margin: 0 0 5px;
        position: relative;
        text-transform: capitalize;

        a{
          color: #a0a0a0;
          text-decoration: none;
          @include prefixer( transition, all 0.4s ease 0s, spec webkit moz o ms );

          &:hover{
            color: $colorPrimary;
          }
        }
      }
    }
    ul.social-icons{
      li{
        display: inline-block;
        width: auto;
        a{
          width: 40px;
          height: 40px;
          display: block;
          line-height: 40px;
          text-align: center;
          background: #ffffff;
          color: #2f2f2f;
          font-size: 17px;
          margin: 0 7px 7px 0;
          border-radius: 100%;
          @include prefixer( transition, all 0.4s ease 0s, spec webkit moz o ms );

          &:hover{
            background: $colorPrimary;
            color: #fff;
          }
        }
        .tooltip {
          .tooltip-inner {
            background: $colorPrimary;
            border-radius: 0 !important;
            padding:5px 10px;
            @include prefixer( transition, all 0.5s ease 0s, spec webkit moz o ms );
          }
          .tooltip-arrow { 
            border-top-color:$colorPrimary !important; 
          }
        }
      }
    }
    #mc-embedded-subscribe-form{
      #mail{
        border: 1px solid #484848;
        color: #bababa;
        background: none;
        font-size: 12px;
        font-style: italic;
        height: 35px;
        padding: 0 0 0 5px;
        width: 65%;
        border-radius: 0;
        float: left;
      }
      .btn{
        background: #484848;
        border: none;
        color: #fff;
        float: right;
        font-size: 11px;
        height: 35px;
        margin: 0;
        text-transform: uppercase;
        width: 30%;
        @include prefixer( transition, all 0.3s ease 0s, spec webkit moz o ms );
        border-radius:0;

        &:hover{
          background: $colorPrimary;
        }
      }
    }
  }
  .footer-bottom{
    padding: 25px 0;
    text-align: center;
    background-color: $bg_color_footer_bottom;
    background-image: url({{ 'bg_footer_top.png' | asset_url }}) no-repeat;
    hr{
      display:none;
    }

    .payment{
      margin-bottom: 15px;

      img{
        display: initial;
        text-align:center;
      }
    }
    .site-footer__links{
      line-height: 22px;
      font-style: normal;
      color: #666666 !important;
      font-size: 13px;
      opacity: 1;
      visibility: visible;
      @include prefixer( transition, opacity 0.24s ease-in-out, spec webkit moz o ms );

      a{
        text-decoration: none;
        @include prefixer( transition, opacity 0.4s ease-in-out, spec webkit moz o ms );

        &:first-child{
          color: $colorPrimary;
        }
        &:last-child{
          color: #666666 !important;
        }
      }
    }
  }
}

